import java.util.ArrayList;


public class PrimeFactorization {

	public static ArrayList<Integer> primeFactors(int i) {
		ArrayList<Integer> ret = new ArrayList<Integer>();
		for(int j = 1; j < i; j++){
			System.out.println(j);
			while(i%j == 0){
				ret.add(j);
				i = i/j;
			}
		}
		
		return ret;
	}

}
